{
	"parser": "babel-eslint",
    "extends": "airbnb",
	"env": {
		"browser": true,
		"node": true
	}, 
	"rules": {
		"react/prop-types": [0],
		"react/jsx-filename-extension": [0],
    "jsx-quotes": [2, "prefer-single"],

		"no-underscore-dangle": ["error", {
			"allow": ["__REDUX_DEVTOOLS_EXTENSION__"],
		}],
		"jsx-a11y/anchor-is-valid": [ "error", {
			"specialLink": [ "Link" ],
		}],
	},
}
